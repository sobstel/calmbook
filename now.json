{
  "version": 2,
  "alias": ["calmbook.page"],
  "builds": [
    {
      "src": "./*.ts",
      "use": "@now/node@canary"
    },
    {
      "src": "./assets/favicon.ico",
      "use": "@now/static"
    },
    {
      "src": "./assets/embed.js",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/assets/favicon.ico",
      "headers": {
        "Cache-Control": "maxage=2592000, s-max-age=2592000"
      }
    },
    {
      "src": "/embed.js",
      "dest": "/assets/embed.js",
      "headers": {
        "Cache-Control": "maxage=2592000, s-max-age=2592000"
      }
    },
    {
      "src": "/__v",
      "dest": "/v.ts",
      "headers": {
        "Cache-Control": "maxage=2592000, s-max-age=2592000"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.ts",
      "headers": {
        "Cache-Control": "max-age=300, s-maxage=3600, stale-if-error=2592000"
      }
    }
  ]
}
